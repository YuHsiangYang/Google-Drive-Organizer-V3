<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Google_Drive_Organizer_V3"
        xmlns:Pages="clr-namespace:Google_Drive_Organizer_V3.Pages" xmlns:Controls="clr-namespace:Google_Drive_Organizer_V3.Controls" x:Class="Google_Drive_Organizer_V3.GoogleDriveOrganizer_MainUI"
        mc:Ignorable="d"
        Title="Google Drive Organizer" Height="441" Width="667" MinHeight="620" MinWidth="667">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="55"/>
        </Grid.RowDefinitions>
        <Grid Name="MainWindow_Grid">
            <Pages:SelectFolderPage x:Name="SelectFolder_Control" HorizontalAlignment="Stretch" Margin="0" VerticalAlignment="Stretch"/>
        </Grid>
        <Grid Name="Controller" Grid.Row="1">
            <Pages:StageNavigationController x:Name="NavController" Grid.Row="1" VerticalAlignment="Stretch"/>
        </Grid>
        <!--<Controls:CircleSwitch></Controls:CircleSwitch>-->
    </Grid>
</Window>
