<UserControl x:Class="Google_Drive_Organizer_V3.Pages.MatchItem.Search_By_PhotoTakenTime"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Google_Drive_Organizer_V3.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="26.562" d:DesignWidth="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Name="Search_Date_StackPanel">
            <ComboBox HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Center" Width="68" x:Name="Search_Input_Year" SelectionChanged="Search_Input_Year_SelectionChanged"/>
            <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="68" x:Name="Search_Input_Month" SelectionChanged="Search_Input_Year_SelectionChanged" Margin="5,0,0,0"/>
            <ComboBox HorizontalAlignment="Left" VerticalAlignment="Center" Width="68" x:Name="Search_Input_Day" SelectionChanged="Search_Input_Year_SelectionChanged" Margin="5,0,0,0"/>
        </StackPanel>
    </Grid>
</UserControl>
