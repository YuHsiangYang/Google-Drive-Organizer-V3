<UserControl x:Class="Google_Drive_Organizer_V3.Pages.MatchItem.Display_types.Match_Display_Icon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Google_Drive_Organizer_V3.Pages.MatchItem.Display_types"
             mc:Ignorable="d" 
             d:DesignHeight="130" d:DesignWidth="120">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10*"></RowDefinition>
            <RowDefinition Height="2*"></RowDefinition>
        </Grid.RowDefinitions>
        <Canvas Grid.Row="0" x:Name="DisplayImage"></Canvas>
        <Viewbox Grid.Row="1" >
            <Label Name="ImageFileName" Content="Label"></Label>
        </Viewbox>
    </Grid>
</UserControl>
